#!/bin/tcsh -x

# check if the DIM interface is already running ...

set r=`ps -ef | grep -i "na62-farm-dim" | grep -v grep | awk '{print $1}'`

# if this is not the case ... start it
if ($r == "") then
    setenv DIM_DNS_NODE lkrpn2.cern.ch
    /usr/local/bin/na62-farm-dim-interface >& dim-interface.log &
endif
